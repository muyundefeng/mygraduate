<script type="text/javascript">
function loadScript(sUrl) {
		var _script = document.createElement('script');
		_script.setAttribute('type', 'text/javascript');
		_script.setAttribute('src', sUrl);
		document.getElementsByTagName('head')[0].appendChild(_script);
}
function Request() {
		loadScript('http://news.open.qq.com/qrcode/gen.php?url='+ARTICLE_INFO.article_url+'&callback=jsonpCallback');
		
}
function jsonpCallback(data)
{
    var aaa = document.getElementById('ewm'); 
    var img = document.getElementById('ewmimg');
     img.src= data.url; 
}
 
	(function(){
	 
 	document.getElementById("s_ewm").onclick = function(){
	  Request();
 		document.getElementById("wemcn").style.display="block";
 	}
 	document.getElementById("ewmkg").onclick = function(){
 		document.getElementById("wemcn").style.display="none";
 	}
    
document.body.onclick = function(e){
var e = e || window.event;
var target = e.target || e.srcElement;
if(target.id == 's_ewm'){
document.getElementById("wemcn").style.display="block";
}else{
document.getElementById("wemcn").style.display="none";
 	}
}
 })()
</script>














